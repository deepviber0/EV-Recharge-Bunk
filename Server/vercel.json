{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "Client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/userlogin",
      "dest": "/Client/userlogin.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Client/index.html"
    }
  ]
}
